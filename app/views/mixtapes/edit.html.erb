<h3>Add More Tracks</h3>

<%= render 'add_track_form' %>

<%= render 'mixtape_name_form', button_text: 'Save Changes' %>

<%= render 'mixtape_tracks' %>

<h3>{{title}}</h3>
<div class="track-listing">
  <% @tracks.each do |track| %>
    <%= render 'track', mustache: track.as_json %>
  <% end %>
</div>  
<script type='text/template' id='playlist-template'>
  <%= render 'track' %>
</script>
