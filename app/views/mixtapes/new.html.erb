<h3>Add A Song</h3>
<%= form_for [@mixtape, @track] do |form| %>
  <%= form.label :artist %>
  <%= form.text_field :artist %>
<br>
  <%= form.label :song %>
  <%= form.text_field :song %>

  <%= form.button 'Add Track to Mixtape' %>
<% end %>

<br>
<p></p>
<%= render 'form', button_text: 'Create Mixtape' %>
  
  <h3><%= @mixtape.id %> <%= @mixtape.title %></h3>

  <h4 class='artist_name'>Artist name goes here</h4>
  <h4 class='song_title'>Song name goes here</h4>




